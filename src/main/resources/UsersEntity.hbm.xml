<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.work.usersEntity.UsersEntity" table="users" schema="tests">
        <id name="idUsers">
            <column name="idUsers" sql-type="int(11)"/>
        </id>
        <property name="nameUsers">
            <column name="nameUsers" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="role">
            <column name="role" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>